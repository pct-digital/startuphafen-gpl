  "scripts": {
    "psql": "docker run  -it --rm --net=host postgres:15.10-bookworm psql postgresql://app:app@database:5432/app",
    "front": "nx serve startuphafen-frontend",
    "back": "nx run startuphafen-backend:serve:development",
    "migrate": "npx prisma migrate dev",
    "entities": "nx run startuphafen-backend:entities --migrationMode=push",
    "test": "nx run-many --target=test,lint,typecheck --parallel=2",
    "e2e": "nx run startuphafen-frontend-e2e:test-accept --headed --noDockerLog",
    "e2e-ci": "nx run startuphafen-frontend-e2e:test-accept --skipBuild --noDockerLog",
    "clean": "rm docker.log -rf; rm dist -rf; rm ./node_modules/.cache/nx -rf; rm coverage -rf; rm .angular/cache -rf; npx nx reset;",
    "testdeploy": "node 'node_modules/.bin/jest' --silent './tools/deploy2/deploy-worker.spec.js' -c './tools/deploy2/jest.config.ts'",
    "build": "bash tools/build.sh"
  },
  "prisma": {
    "schema": "apps/startuphafen-backend/src/assets/prisma/schema.prisma"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "src/e2e/*.cy.ts"
