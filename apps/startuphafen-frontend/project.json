{
  "name": "startuphafen-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "sh",
  "sourceRoot": "apps/startuphafen-frontend/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "allowedCommonJsDependencies": [
          "js-sha256",
          "lodash",
          "jszip",
          "socket.io-client",
          "seedrandom",
          "assert",
          "slugify",
          "ejson"
        ],
        "outputPath": "dist/apps/startuphafen-frontend",
        "index": "apps/startuphafen-frontend/src/index.html",
        "main": "apps/startuphafen-frontend/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/startuphafen-frontend/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/startuphafen-frontend/src/assets"
          },
          {
            "glob": "**/**",
            "input": "libs/angular-common/src/assets",
            "output": "./assets"
          }
        ],
        "styles": ["apps/startuphafen-frontend/src/styles.scss"],
        "scripts": [],
        "budgets": [
          {
            "type": "initial",
            "maximumWarning": "9mb",
            "maximumError": "10mb"
          },
          {
            "type": "anyComponentStyle",
            "maximumWarning": "2kb",
            "maximumError": "8kb"
          }
        ]
      },
      "configurations": {
        "production": {
          "outputHashing": "all",
          "styles": [
            "apps/startuphafen-frontend/src/styles.scss",
            {
              "input": "libs/angular-common/src/environments/variables.scss"
            }
          ],
          "assets": [
            "apps/startuphafen-frontend/src/assets",
            {
              "glob": "**/**",
              "input": "libs/angular-common/src/assets",
              "output": "./assets"
            }
          ],
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "4mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "8kb"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "styles": [
            "apps/startuphafen-frontend/src/styles.scss",
            {
              "input": "libs/angular-common/src/environments/variables.scss"
            }
          ],
          "assets": [
            "apps/startuphafen-frontend/src/assets",
            {
              "glob": "**/**",
              "input": "libs/angular-common/src/assets",
              "output": "./assets"
            }
          ]
        }
      },
      "defaultConfiguration": "development"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "startuphafen-frontend:build:production",
          "disableHostCheck": true
        },
        "development": {
          "host": "0.0.0.0",
          "port": 4201,
          "disableHostCheck": true,
          "buildTarget": "startuphafen-frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "startuphafen-frontend:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/startuphafen-frontend/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "startuphafen-frontend:build"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/startuphafen-frontend",
        "commands": [
          "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.spec.json --noEmit"
        ]
      }
    },
    "doc": {
      "executor": "@startuphafen/pct-pipeline:doc-builder",
      "options": {
        "app": "startuphafen-frontend"
      }
    }
  }
}
