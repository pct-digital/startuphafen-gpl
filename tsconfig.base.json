{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "noPropertyAccessFromIndexSignature": false,
    "declaration": false,
    "strict": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "module": "es2022",
    "lib": ["ES2022", "dom"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "paths": {
