        "libs/ui/ui-formfield-helm/src/index.ts"
      ],
      "@spartan-ng/ui-icon-helm": ["libs/ui/ui-icon-helm/src/index.ts"],
      "@spartan-ng/ui-input-helm": ["libs/ui/ui-input-helm/src/index.ts"],
      "@spartan-ng/ui-pagination-helm": [
        "libs/ui/ui-pagination-helm/src/index.ts"
      ],
      "@spartan-ng/ui-separator-helm": [
        "libs/ui/ui-separator-helm/ui-separator-helm/src/index.ts"
      ],
      "@startuphafen/angular-common": ["libs/angular-common/src/index.ts"],
      "@startuphafen/base64": ["libs/base64/src/index.ts"],
      "@startuphafen/cypress-help": ["libs/cypress-help/src/index.ts"],
      "@startuphafen/db-migration": ["libs/db-migration/src/index.ts"],
      "@startuphafen/dockerized-node-libs": [
        "libs/dockerized-node-libs/src/index.ts"
      ],
      "@startuphafen/enitity-engine/server-entity-engine": [
        "libs/entity-engine/server/src/index.ts"
      ],
      "@startuphafen/entity-engine/common": [
        "libs/entity-engine/common/src/index.ts"
      ],
      "@startuphafen/eric-connector": ["libs/eric-connector/src/index.ts"],
      "@startuphafen/pct-pipeline": ["libs/pct-pipeline/src/index.ts"],
      "@startuphafen/serialized-transaction": [
        "libs/serialized-transaction/src/index.ts"
      ],
      "@startuphafen/server": ["libs/watermark/server/src/index.ts"],
      "@startuphafen/spectator-help": ["libs/spectator-help/src/index.ts"],
      "@startuphafen/startuphafen-common": [
