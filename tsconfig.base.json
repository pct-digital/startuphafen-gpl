      "@startuphafen/utility": ["libs/utility/src/index.ts"],
      "@startuphafen/utility-server": ["libs/utility-server/src/index.ts"],
      "@startuphafen/watermark/angular": [
        "libs/watermark/angular/src/index.ts"
      ],
      "@startuphafen/watermark/server": ["libs/watermark/server/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
